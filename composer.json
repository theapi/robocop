{
    "name": "theapi/robocop",
    "keywords":     ["CCTV", "Symfony2"],
    "homepage":     "https://github.com/theapi/robocop",
    "authors":      [
        {
            "name": "Peter Clarke"
        }
    ],

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "php-mime-mail-parser/php-mime-mail-parser",
                "version": "dev-master",
                "source": {
                    "url": "http://php-mime-mail-parser.googlecode.com/svn/",
                    "type": "svn",
                    "reference": "trunk/"
                },
                "autoload": {
                    "classmap": ["MimeMailParser.class.php"]
                }
            }
        }
    ],

    "require": {
        "php":                           ">=5.3.1",
        "symfony/console":               ">=2.0,<2.3-dev",
        "symfony/config":                ">=2.0,<2.3-dev",
        "symfony/dependency-injection":  ">=2.0,<2.3-dev",
        "symfony/swiftmailer-bundle":    "2.2.*",
        "symfony/yaml":                  ">=2.0,<2.3-dev",
        "php-mime-mail-parser/php-mime-mail-parser": "dev-master"
    },
    
    "autoload": {
        "psr-0": {"Theapi\\Robocop": "src/"}
    }
}
